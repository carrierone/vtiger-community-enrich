{
  "manifest_version": 3,
  "name": "vTiger Community Enrich",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "16": "assets/images/vtiger16.png",
    "48": "assets/images/vtiger48.png",
    "128": "assets/images/vtiger128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
