{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "assets/images/vtiger16.png",
    "48": "assets/images/vtiger48.png",
    "128": "assets/images/vtiger128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
